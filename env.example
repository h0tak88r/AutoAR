# AutoAR Environment Configuration
# Copy this file to .env and update with your values

# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_WEBHOOK=your_discord_webhook_url_here

# Database Configuration
# Option 1: Use PostgreSQL connection string
DB_TYPE=postgresql
DB_HOST=postgresql://username:password@host:port/database

# Option 2: Use individual environment variables
# DB_TYPE=postgresql
# DB_HOST=your_postgres_host
# DB_PORT=5432
# DB_USER=your_postgres_user
# DB_PASSWORD=your_postgres_password
# DB_NAME=your_postgres_database

# Example for your external database:
# DB_HOST=postgresql://sallam:cyhXor-myfsuk-jobbo9@174.138.80.170:5432/bughunt
# DB_PORT=5432
# DB_USER=sallam
# DB_PASSWORD=cyhXor-myfsuk-jobbo9
# DB_NAME=bughunt

# Application Configuration
DISCORD_ONLY=false
RUN_SETUP=false
REGENERATE_CONFIG=true
SAVE_TO_DB=true
VERBOSE=true

# API Keys (optional)
GITHUB_TOKEN=your_github_token_here
SECURITYTRAILS_API_KEY=your_securitytrails_key_here
SHODAN_API_KEY=your_shodan_key_here
VIRUSTOTAL_API_KEY=your_virustotal_key_here

# Add other API keys as needed...
